<% provide :title, 'New Contact' %>

<%= form_with model: [@episode, @contact], local: true do |f| %>
  <div class="input-field">
    <%= f.label :body %>
    <%= f.text_field :body, id: "contact_body" %>
  </div>
  <%= button_tag class: 'btn' do %>
    <span>
      Create Contact
      <i class='material-icons right'>send</i>
    <span>
  <% end %>
<% end %>