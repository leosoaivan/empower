<% provide(:title, "Login") %>

<h1>Welcome to Empower</h1>
<h2>system reform advocacy</h2>

<%= form_with url: login_path, local: true do |f| %>
  <% f.label :username %>
  <%= f.text_field :username, placeholder: "username" %>

  <% f.label :password %>
  <%= f.password_field :password, placeholder: "password" %>

  <%= f.submit "LOGIN" %>
<% end %>