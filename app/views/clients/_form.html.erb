<%= form_with model: @client, local: true do |form| %>
  <%= form.text_field :firstname, id: 'firstname', placeholder: 'first name' %>
  <%= form.text_field :lastname, id: 'lastname', placeholder: 'last name' %>
  <%= form.date_field :dob, id: 'dob', placeholder: 'mm/dd/yyyy'%>
  <%= form.text_field :telephone, id: 'telephone', placeholder: 'telephone' %>
  <%= form.submit form_submit_action %>
<% end %>